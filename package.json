{
    "name": "ksmf-web-react",
    "version": "1.0.0",
    "description": "Template to create web application based on React, Material UI and KsMf",
    "main": "node server/bin/server.js",
    "scripts": {
      "start": "node server/bin/server.js",
      "build": "cd client && npm install && npm run build",
      "test": "npm run server:test && npm run client:test",
      "dev": "nodemon server/bin/server.js",
      "client:start": "cd client && npm start",
      "client:build": "cd client && npm run build",
      "client:watch": "cd client && npm run watch",
      "client:test": "cd client && npm run test",
      "server:test": "jest /server"
    },
    "keywords": [
      "ksmf",
      "rest",
      "api",
      "web",
      "ksike",
      "react",
      "material"
    ],
    "author": {
      "name": "Antonio Membrides Espinosa",
      "email": "tonykssa@gmail.com",
      "url": "https://github.com/ameksike"
    },
    "license": "ISC",
    "dependencies": {
      "axios": "^0.24.0",
      "ksmf": "^1.0.51"
    },
    "devDependencies": {
      "jest": "^26.6.3",
      "supertest": "^6.1.3",
      "nodemon": "^2.0.15"
    }
}
  